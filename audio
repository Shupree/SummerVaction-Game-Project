// btn1을 눌렀을 때 MP_Fearful Situation 재생
document.querySelector(".btn1").addEventListener("click", function () {
    var audio1 = new Audio("c:\Users\USER\Desktop\효과음\bgm\MP_Fearful Situation.mp3");
    audio1.loop = false; // 반복재생하지 않음
    audio1.volume = 0.5; // 음량 설정
    audio1.play(); // MP_Fearful Situation 재생
  });